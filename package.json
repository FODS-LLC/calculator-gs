{
  "scripts": {
    "serve": "cross-env NODE_ENV=development concurrently \"postcss public/css/main.css -o public/build/tailwind.css --watch\"  \"live-server ./public\"",
    "development": "cross-env NODE_ENV=development postcss public/css/main.css -o public/build/tailwind.css",
    "production": "cross-env NODE_ENV=production postcss public/css/main.css -o public/build/tailwind.css",
    "start": "npm run serve"
  },
  "dependencies": {
    "autoprefixer": "^10.2.5",
    "aws-sdk": "^2.905.0",
    "dotenv": "^9.0.1",
    "google-spreadsheet": "^3.1.15",
    "googleapis": "^39.2.0",
    "netlify": "^6.1.27",
    "postcss": "^8.2.14",
    "postcss-import": "^14.0.1",
    "tailwindcss": "^2.1.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "live-server": "^1.2.1",
    "postcss-cli": "^8.3.1"
  }
}
